{
  "name": "ctot",
  "description": "Convert CoffeeScript to TypeScript",
  "keywords": [
    "javascript",
    "typescript",
    "language",
    "coffeescript",
    "compiler"
  ],
  "author": "Bernd Helzer",
  "version": "0.9",
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "lib": "./lib/coffee-script"
  },
  "main": "./lib/coffee-script/coffee-script",
  "bin": {
    "ctot": "./bin/ctot",
    "cake": "./bin/cake"
  },
  "preferGlobal": false,
  "scripts": {
    "test":         "node runtest.js",
    "build":        "coffee -o lib/coffee-script -w -c src/*.coffee"
  },
  "homepage": "http://coffeescript.org",
  "bugs": "https://github.com/jashkenas/coffeescript/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jashkenas/coffeescript.git"
  },
  "devDependencies": {
    "uglify-js": "~2.2",
    "jison": ">=0.2.0",
    "highlight.js": "~8.0.0",
    "underscore": "~1.5.2",
    "docco": "~0.7.0"
  }
}
